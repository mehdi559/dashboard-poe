{
 "name": "dashboard-poe",
 "version": "0.1.2",
 "private": true,
 "homepage": "./",
 "description": "Dashboard POE - Gestionnaire Financier Personnel pour suivi budg√©taire",
 "author": "Votre Nom <noreplay@email.com>",
 "dependencies": {
  "@testing-library/dom": "^10.4.0",
  "@testing-library/jest-dom": "^6.6.3",
  "@testing-library/react": "^16.3.0",
  "@testing-library/user-event": "^13.5.0",
  "date-fns": "^4.1.0",
  "exceljs": "^4.4.0",
  "html2canvas": "^1.4.1",
  "html2pdf.js": "^0.10.3",
  "jspdf": "^3.0.1",
  "lodash.clonedeep": "^4.5.0",
  "lucide-react": "^0.525.0",
  "react": "^19.1.0",
  "react-datepicker": "^8.4.0",
  "react-dom": "^19.1.0",
  "react-router-dom": "^7.6.3",
  "react-scripts": "5.0.1",
  "recharts": "^3.0.2",
  "web-vitals": "^2.1.4",
  "xlsx": "^0.18.5"
 },
 "main": "public/electron.js",
 "scripts": {
  "start": "react-scripts start",
  "build": "react-scripts build",
  "test": "react-scripts test",
  "eject": "react-scripts eject",
  "electron": "electron .",
  "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
  "electron-pack": "npm run build && electron-builder",
  "electron-pack-mac": "npm run build && electron-builder --mac",
  "electron-pack-win": "npm run build && electron-builder --win",
  "electron-pack-linux": "npm run build && electron-builder --linux",
  "electron-pack-all": "npm run build && electron-builder -mwl",
  "build-etsy": "node build-etsy.js"
 },
 "eslintConfig": {
  "extends": [
   "react-app",
   "react-app/jest"
  ]
 },
 "browserslist": {
  "production": [
   ">0.2%",
   "not dead",
   "not op_mini all"
  ],
  "development": [
   "last 1 chrome version",
   "last 1 firefox version",
   "last 1 safari version"
  ]
 },
 "devDependencies": {
  "autoprefixer": "^10.4.21",
  "concurrently": "^9.2.0",
  "electron": "^37.2.4",
  "electron-builder": "^26.0.12",
  "electron-is-dev": "^3.0.1",
  "electron-packager": "^17.1.2",
  "postcss": "^8.5.6",
  "tailwindcss": "^3.4.17",
  "wait-on": "^8.0.3"
 },
 "build": {
  "appId": "com.dashboardpoe.app",
  "productName": "Dashboard POE - Gestionnaire Financier",
  "directories": {
   "output": "dist"
  },
  "files": [
   "build/**/*",
   "public/electron.js",
   "public/preload.js",
   "public/logo192.png",
   "public/logo512.png"
  ],
  "mac": {
   "category": "public.app-category.finance",
   "icon": "public/logo192.png",
   "target": [
    {
     "target": "zip",
     "arch": [
      "x64",
      "arm64"
     ]
    }
   ],
   "hardenedRuntime": true,
   "gatekeeperAssess": false,
   "entitlements": "public/entitlements.mac.plist",
   "entitlementsInherit": "public/entitlements.mac.plist"
  },
  "win": {
   "icon": "public/logo192.png",
   "target": [
    {
     "target": "zip",
     "arch": [
      "x64"
     ]
    }
   ],
   "requestedExecutionLevel": "asInvoker"
  },
  "linux": {
   "icon": "public/logo192.png",
   "target": [
    {
     "target": "zip",
     "arch": [
      "x64"
     ]
    }
   ],
   "category": "Finance"
  },
  "compression": "maximum",
  "removePackageScripts": true,
  "removePackageKeywords": true,
  "artifactName": "${productName}-${version}-${platform}-${arch}.${ext}",
  "extraMetadata": {}
 }
}
